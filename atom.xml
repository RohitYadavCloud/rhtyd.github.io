---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Rohit Yadav</title>
    <link href="http://bhaisaab.org/atom.xml" rel="self"/>
    <link href="http://bhaisaab.org/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://bhaisaab.org/</id>

    <author>
      <name>Rohit Yadav</name>
      <email>rohityadav89@gmail.com</email>
    </author>

    {% for post in site.posts %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="http://bhaisaab.org{{ post.url }}"/>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>http://bhaisaab.org{{ post.id }}</id>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>
